AAA,cAAA,CACA,8BADA,EAEA,mCAFA,CAAA;AAIA,SAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,YAAA,CAAA,IAAA,CAAA,KAAA;;AACA,UAAA,YAAA,CACA,wBADA,EAEA,iCAFA,CAAA;AAIA,SAfA;AAgBA;AACA,KAnCA;AAoCA,IAAA,aApCA,yBAoCA,IApCA,EAoCA;AACA,UAAA,gDAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA;;AACA,aAAA,KAAA;AACA;AAzCA;AAVA,CAAA","sourcesContent":["<template>\n  <div>\n    <form id=\"productCU\" @submit.prevent=\"createAddress\">\n      <h4 class=\"text-center\">Create Address</h4>\n      <div\n        class=\"alert alert-danger\"\n        role=\"alert\"\n        v-for=\"(index, error) in errorMessage\"\n        :key=\"index\"\n      >\n        {{ error }}\n      </div>\n      <div class=\"form-group\">\n        <label for=\"firstName\"\n          >First-Name<span class=\"text-danger\">*</span></label\n        >\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"firstName\"\n          v-model=\"address.firstName\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"lastName\"\n          >Last Name<span class=\"text-danger\">*</span></label\n        >\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"lastName\"\n          v-model=\"address.lastName\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"email\">E-mail<span class=\"text-danger\">*</span></label>\n        <input\n          type=\"email\"\n          class=\"form-control\"\n          id=\"email\"\n          v-model=\"address.email\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"country\">Country<span class=\"text-danger\">*</span></label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"country\"\n          v-model=\"address.country\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"city\">City</label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"city\"\n          v-model=\"address.city\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"district\">District<span class=\"text-danger\">*</span></label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"district\"\n          v-model=\"address.district\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"zip-code\"\n          >Postal / Zip Code<span class=\"text-danger\">*</span></label\n        >\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"zip-code\"\n          v-model=\"address.zipCode\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"address\"\n          >Address / Location<span class=\"text-danger\">*</span></label\n        >\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"address\"\n          name=\"address\"\n          v-model=\"address.addresses\"\n          required\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"phone\"\n          >Phone Number<span class=\"text-danger\">*</span></label\n        >\n        <input\n          type=\"tel\"\n          class=\"form-control\"\n          id=\"phone\"\n          name=\"phone\"\n          v-model=\"address.phoneNumber\"\n          required\n        />\n      </div>\n      <small id=\"emailHelp\" class=\"form-text text-muted\"\n        >We'll never share your data with anyone else.</small\n      >\n      <br />\n    </form>\n    <button type=\"submit\" class=\"btn btn-primary mx-auto\">\n      <i class=\"fa fa-spinner fa-spin mr-1\" v-if=\"showLoader\"></i>Save Address\n    </button>\n  </div>\n</template>\n<script>\n  /* eslint-disable */\nimport {\n  errorToaster,\n  successToaster\n} from \"../../shared/service/ErrorHandler\";\n\nexport default {\n  data() {\n    return {\n      address: {\n        phoneNumber: \"\"\n      },\n      errorMessage: [],\n      showLoader: false\n    };\n  },\n  methods: {\n    createAddress() {\n      if (this.address.firstName.length < 5) {\n        this.errorMessage.push(\n          \"FirstName should contains more than 5 character\"\n        );\n      }\n      if (this.ValidateEmail(this.address.email) === false) {\n        this.errorMessage.push(\"Please provide a valid Email address\");\n      }\n      if (this.address.phoneNumber.length < 10) {\n        this.errorMessage.push(\"Please provide valid phone number\");\n      }\n      if (this.errorMessage.length === 0) {\n          this.loader(true,{\n              context:\"creating address...\",\n              area:\"app\"\n          })\n        this.$store.dispatch(\"createAddress\", this.address)\n          .then(response => {\n              this.loader(false)\n            successToaster(\n              \"address created Successfully\",\n              \"User address created Successfully\"\n            );\n          })\n          .catch(error => {\n              this.loader(false)\n              this.errorMessage.push(error)\n            errorToaster(\n              \"address created Failed\",\n              \"Please try again after sometime\"\n            );\n          });\n      }\n    },\n    ValidateEmail(mail) {\n      if (/^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/.test(mail)) {\n        return true;\n      }\n      return false;\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components/products/address"}]}